{
  "name": "react-express",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/pratyush1712/react-express.git",
  "author": "Pratyush Sudhakar <pratyushsudhakar03@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:server": "cd server && yarn start",
    "start:client": "cd client && yarn start",
    "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "build:server": "cd server && yarn build",
    "build:client": "cd client && yarn build",
    "dev:server": "cd server && yarn dev",
    "lint": "yarn eslint . && yarn prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format": "yarn prettier --write \"**/*.{js,jsx,json,css,md,ts,tsx}\"",
    "prettier": "prettier"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.7",
    "typescript": "^5.0.3"
  }
}
